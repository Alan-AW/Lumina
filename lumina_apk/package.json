{
  "name": "Lumina",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --active-arch-only",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "start-no-cache": "npx react-native start --reset-cache",
    "prod": "cd android && ./gradlew assembleRelease && cd ..",
    "prodTest": "npx react-native run-android --variant=release",
    "dev": "npm run icon-reset && npx react-native run-android --active-arch-only",
    "icon-reset": "npx iconfont-rn",
    "initDebug": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "apk": "yarn initDebug && cd android && ./gradlew assembleDebug  && cd ..",
    "clean": "cd android && ./gradlew clean && cd .."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.4",
    "@react-native-community/slider": "^4.4.3",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.16",
    "@reduxjs/toolkit": "^1.9.7",
    "@shopify/react-native-skia": "^0.1.216",
    "@wuba/react-native-echarts": "^1.2.5",
    "axios": "^1.5.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "echarts": "^5.4.3",
    "i18next": "^23.7.6",
    "react": "18.2.0",
    "react-i18next": "^13.4.1",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-iconfont-cli": "^2.2.4",
    "react-native-orientation-locker": "^1.5.0",
    "react-native-permissions": "^3.10.1",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.3",
    "react-native-screens": "^3.27.0",
    "react-native-storage": "^1.0.1",
    "react-native-svg": "^13.14.0",
    "react-native-vision-camera": "^3.6.4",
    "react-native-webview": "^13.6.2",
    "react-redux": "^8.1.3",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
